{
    "name": "dicr/yii2-cache",
    "description": "Cache components for Yii2",
    "license": "MIT",
    "type": "yii2-extension",
    "keywords": ["dicr", "php", "yii2", "cache"],
    "homepage": "https://gitlab.com/dicr/yii2-cache",
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/dicr/yii2-cache.git"
    },
    "autoload": {
        "psr-4": {
            "dicr\\cache\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": "~2.0.35",
        "dicr/yii2-helper": "~2.3.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://gitlab.com/dicr/yii2-helper.git",
            "name": "yii2-helper"
        }, {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
